{
  "kind": "collectionType",
  "collectionName": "Blog",
  "info": {
    "name": "Blog"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Body": {
      "type": "richtext"
    },
    "Image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "Date": {
      "type": "date"
    },
    "Id_Notice": {
      "via": "Id_Notice",
      "collection": "notice"
    },
    "informacaos": {
      "via": "blog",
      "collection": "informacao"
    }
  }
}
